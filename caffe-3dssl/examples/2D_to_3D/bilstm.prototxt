}  

#lstm
layer{
  name: "reshape-data"
  type: "Reshape"
  bottom: "fc"
  top: "fc-reshape"
  reshape_param{
    shape{
      dim: -1
      dim: 1
      dim: 1024
    }
  }
}
layer {
  name: "lstm1"
  type: "LSTM"
  bottom: "fc-reshape"
  bottom: "clip"
  top: "lstm1"
  recurrent_param {
    num_output: 1024
    weight_filler {
      type: "uniform"
      min: -0.01
      max: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer{
  name: "reshape-lstm1"
  type: "Reshape"
  bottom: "lstm1"
  top: "lstm1_reshape"
  reshape_param{
    shape{
      dim: -1
      dim: 1024
    }
  }
}
layer {
  name: "inverted1"
  type: "InvertedOrder"
  top: "lstm1_inverted"
  bottom: "lstm1_reshape"
}
layer{
  name: "reshape-lstm1_"
  type: "Reshape"
  bottom: "lstm1_inverted"
  top: "lstm2_input"
  reshape_param{
    shape{
      dim: -1
      dim: 1
      dim: 1024
    }
  }
}
layer {
  name: "lstm2"
  type: "LSTM"
  bottom: "lstm2_input"
  bottom: "clip"
  top: "lstm2"
  recurrent_param {
    num_output: 1024
    weight_filler {
      type: "uniform"
      min: -0.01
      max: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "fc-final_16"
  type: "InnerProduct"
  bottom: "lstm2"
  top: "fc-final"
  param {
    lr_mult: 10
    decay_mult: 1
  }
  param {
    lr_mult: 20
    decay_mult: 0
  }
  inner_product_param {
    num_output: 48
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer{
  name: "reshape-fc"
  type: "Reshape"
  bottom: "fc-final"
  top: "predict"
  reshape_param{
    shape{
      dim: -1
      dim: 48
    }
  }
}
layer {
  name: "inverted2"
  type: "InvertedOrder"
  top: "predict_inverted"
  bottom: "predict"
}

layer {
  name: "loss"
  type: "EuclideanLoss"
  bottom: "predict_inverted"
  bottom: "label3d"
  top: "loss" 
}

layer {
  name: "tmp"
  type: "EuclideanLoss"
  bottom: "label2d"
  bottom: "label2d"
  top: "tmp"
}

layer {
  name: "evalution"
  type: "MPJPEEvaluation"
  bottom: "predict_inverted"
  bottom: "label"
  top: "evalution"
  loss_weight: 0.0
  mpjpe_param {
#    save_filepath: "results.csv"
    scale: 1
    sample_num: 30
    max_min_normalize: true
    max_min_source: "/data/h36m/16point_mean_limb_scaled_max_min.csv"
  }
}

