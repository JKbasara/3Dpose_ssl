layer {  
  name: "demo"  
  type: "ImageDataLstm"  
  top: "data"  
  top: "label_clip"  
  include {  
    phase: TRAIN  
  }  
  transform_param {  
    scale: 0.00390625  
    mean_value: 128  
  }  
  image_data_lstm_param {  
    source: "/data/h36m/16skel_train_2d3d_clip.txt"  
    root_folder: "Human3.6m_ROOT_DIR"
    new_height: 368  
    new_width: 368  
    is_color: true 
    batch_size: 5  
    shuffle: false
    label_dim: 81 
    is_shuffle_batch: true
  }  
}
layer {  
  name: "demo"  
  type: "ImageDataLstm"  
  top: "data"  
  top: "label_clip"  
  include {  
    phase: TEST
  }  
  transform_param {  
    scale: 0.00390625  
    mean_value: 128  
  }  
  image_data_lstm_param {  
    source: "/data/h36m/16skel_test_2d3d_clip.txt"   
